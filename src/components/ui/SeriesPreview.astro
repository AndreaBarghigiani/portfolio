---
import type { CollectionEntry } from 'astro:content';
interface Props {
  entry: CollectionEntry<'series'>;
}

const { entry } = Astro.props;
---

<li class="py-2">
  <div class="flex gap-5">
    <a href={`/series/${entry.id}/`} class="flex flex-col gap-3">
      <div class="flex flex-col">
        <span class="text-headings text-3xl font-medium">{entry.data.title}</span>
        <p>{entry.data.seo.description}</p>
      </div>
    </a>
  </div>
</li>
